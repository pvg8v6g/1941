[*]
# Most of the standard properties are supported
indent_size = 4
max_line_length = 180

csharp_space_after_cast = true
resharper_csharp_keep_existing_linebreaks = false
insert_final_newline = true
resharper_blank_lines_inside_region = 1
resharper_blank_lines_around_property = 1
resharper_blank_lines_around_auto_property = 1
resharper_blank_lines_around_single_line_property = 1
resharper_blank_lines_around_single_line_auto_property = 1

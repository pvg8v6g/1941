<Window x:Class="UX.Views.MainWindowView.MainWindowView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mainWindowViewModel="clr-namespace:UX.ViewModels.MainWindowViewModel"
        xmlns:behaviors="clr-namespace:Library.Behaviors;assembly=Library"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance mainWindowViewModel:MainWindowViewModel}"
        behaviors:LoadedBehavior.LoadedCommand="{Binding LoadedCommand}"
        Title="1941"
        WindowState="Maximized"
        Background="{StaticResource PrimaryBrush}">
    <DockPanel>
        <!-- <ContentControl DockPanel.Dock="Top" -->
        <!--                 Content="{Binding NavigationService.TopBar}" /> -->
        <ContentControl DockPanel.Dock="Bottom"
                        Content="{Binding NavigationService.ActivePage}" />
    </DockPanel>
</Window>
